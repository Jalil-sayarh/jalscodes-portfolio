{
  "rewrites": [
    {
      "source": "/projects/:project",
      "destination": "/projects/:project/index.html"
    },
    {
      "source": "/projects/:project/:path*",
      "destination": "/projects/:project/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}