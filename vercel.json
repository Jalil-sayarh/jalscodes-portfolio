{
  "rewrites": [
    {
      "source": "/projects/([^/]+)/?$",
      "destination": "/projects/$1/index.html"
    },
    {
      "source": "/projects/([^/]+)/(.*)",
      "destination": "/projects/$1/$2"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}